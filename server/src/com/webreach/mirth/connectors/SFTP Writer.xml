<connectorMetaData>
  <name>SFTP Writer</name>
  <author>WebReach, Inc.</author>
  <pluginVersion>@mirthversion</pluginVersion>
  <mirthVersion>1.7.0</mirthVersion>
  <updateUrl><![CDATA[http://extensions.mirthproject.org/updates/?mirthversion=${mirthVersion}&version=${version}&name=${name}&serverid=${serverid}]]></updateUrl>
  <url>http://extensions.mirthproject.org</url>
  <description>This connector allows Mirth to write files to a SFTP server.</description>
  <clientClassName>com.webreach.mirth.connectors.sftp.SFTPWriter</clientClassName>
  <serverClassName>com.webreach.mirth.connectors.sftp.SftpConnector</serverClassName>
  <sharedClassName>com.webreach.mirth.connectors.sftp.SFTPWriterProperties</sharedClassName>
  <transformers></transformers>
  <protocol>sftp</protocol>
  <type>SENDER</type>
  <enabled>true</enabled>
  <mule-properties>
      <property name="connector">com.webreach.mirth.connectors.sftp.SftpConnector</property>
      <property name="dispatcher.factory">com.webreach.mirth.connectors.sftp.SftpMessageDispatcherFactory</property>
      <property name="message.receiver">com.webreach.mirth.connectors.sftp.SftpMessageReceiver</property>
      <property name="message.adapter">com.webreach.mirth.connectors.sftp.SftpMessageAdapter</property>
      <property name="endpoint.builder">org.mule.impl.endpoint.SocketEndpointBuilder</property>
  </mule-properties>
</connectorMetaData>