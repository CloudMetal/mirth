<connectorMetaData path="dicom">
	<name>DICOM Sender</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1-1.8</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This connector allows Mirth to send messages using DICOM protocol.</description>
	<clientClassName>com.webreach.mirth.connectors.dimse.DICOMSender</clientClassName>
	<serverClassName>com.webreach.mirth.connectors.dimse.DICOMConnector</serverClassName>
	<sharedClassName>com.webreach.mirth.connectors.dimse.DICOMSenderProperties</sharedClassName>
	<library path="dicom-client.jar">
		<type>CLIENT</type>
	</library>
	<library path="dicom-shared.jar">
		<type>SHARED</type>
	</library>
	<library path="lib/slf4j-api-1.2.jar">
		<type>SHARED</type>
	</library>	
	<library path="lib/slf4j-log4j12-1.2.jar">
		<type>SHARED</type>
	</library>
	<library path="lib/dcm4che-core-2.0.15.jar">
		<type>SHARED</type>
	</library>
	<library path="dicom-server.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/jai_imageio.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/dcm4che-net-2.0.15.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/dcm4che-tool-dcmrcv-2.0.15.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/dcm4che-tool-dcmsnd-2.0.15.jar">
		<type>SERVER</type>
	</library>
	<transformers></transformers>
	<protocol>dicom</protocol>
	<type>DESTINATION</type>
	<enabled>true</enabled>
	<mule-properties>
		<property name="connector">com.webreach.mirth.connectors.dimse.DICOMConnector</property>
		<property name="dispatcher.factory">com.webreach.mirth.connectors.dimse.DICOMMessageDispatcherFactory</property>
		<property name="message.receiver">com.webreach.mirth.connectors.dimse.DICOMMessageReceiver</property>
		<property name="message.adapter">com.webreach.mirth.connectors.dimse.DICOMMessageAdapter</property>
		<property name="endpoint.builder">com.webreach.mirth.connectors.file.FileEndpointBuilder</property>
	</mule-properties>
</connectorMetaData>