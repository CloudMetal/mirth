<connectorMetaData path="soap">
	<name>SOAP Sender</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This connector allows Mirth to invoke remote SOAP WebServices over HTTP.</description>
	<clientClassName>com.webreach.mirth.connectors.soap.SOAPSender</clientClassName>
	<serverClassName>com.webreach.mirth.connectors.soap.axis.AxisConnector</serverClassName>
	<sharedClassName>com.webreach.mirth.connectors.soap.SOAPSenderProperties</sharedClassName>
	<serviceClassName>com.webreach.mirth.connectors.soap.SoapConnectorService</serviceClassName>
	<library path="soap-client.jar">
		<type>CLIENT</type>
	</library>
	<library path="soap-shared.jar">
		<type>SHARED</type>
	</library>
	<library path="soap-server.jar">
		<type>SERVER</type>
	</library>
	<transformers></transformers>
	<protocol>axis</protocol>
	<type>DESTINATION</type>
	<enabled>true</enabled>
	<mule-properties>
		<property name="connector">com.webreach.mirth.connectors.soap.axis.AxisConnector</property>
		<property name="dispatcher.factory">com.webreach.mirth.connectors.soap.axis.AxisMessageDispatcherFactory</property>
		<property name="message.receiver">com.webreach.mirth.connectors.soap.axis.AxisMessageReceiver</property>
		<property name="message.adapter">com.webreach.mirth.connectors.soap.axis.AxisMessageAdapter</property>
		<property name="inbound.transformer"></property>
		<property name="outbound.transformer"></property>
		<property name="endpoint.builder">org.mule.impl.endpoint.UrlEndpointBuilder</property>
	</mule-properties>
</connectorMetaData>