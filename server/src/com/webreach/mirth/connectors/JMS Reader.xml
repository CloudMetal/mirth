<connectorMetaData>
  <name>JMS Reader</name>
  <author>WebReach, Inc.</author>
  <pluginVersion>@mirthversion</pluginVersion>
  <mirthVersion>1.7.1</mirthVersion>
  <updateUrl><![CDATA[http://extensions.mirthproject.org/updates/?mirthversion=${mirthVersion}&version=${version}&name=${name}&serverid=${serverid}]]></updateUrl>
  <url>http://extensions.mirthproject.org</url>
  <description>This connector allows Mirth to read messages from a JMS queue.</description>
  <clientClassName>com.webreach.mirth.connectors.jms.JMSReader</clientClassName>
  <serverClassName>com.webreach.mirth.connectors.jms.JmsConnector</serverClassName>
  <sharedClassName>com.webreach.mirth.connectors.jms.JMSReaderProperties</sharedClassName>
  <transformers>JMSMessageToObject ObjectToString</transformers>
  <protocol>jms</protocol>
  <type>LISTENER</type>
  <enabled>true</enabled>
  <mule-properties>
      <property name="connector">com.webreach.mirth.connectors.jms.JmsConnector</property>
      <property name="dispatcher.factory">com.webreach.mirth.connectors.jms.JmsMessageDispatcherFactory</property>
      <property name="message.receiver">com.webreach.mirth.connectors.jms.JmsMessageReceiver</property>
      <property name="transacted.message.receiver">com.webreach.mirth.connectors.jms.TransactedJmsMessageReceiver</property>
      <property name="message.adapter">com.webreach.mirth.connectors.jms.JmsMessageAdapter</property>
      <property name="inbound.transformer">com.webreach.mirth.connectors.jms.transformers.JMSMessageToObject</property>
      <property name="response.transformer">com.webreach.mirth.connectors.jms.transformers.JMSMessageToObject</property>
      <property name="outbound.transformer">com.webreach.mirth.connectors.jms.transformers.ObjectToJMSMessage</property>
      <property name="service.error">Cannot create Jms connector using current service descriptor</property>
      <property name="endpoint.builder">org.mule.impl.endpoint.ResourceNameEndpointBuilder</property>
   </mule-properties>
</connectorMetaData>