<connectorMetaData path="com.webreach.mirth.connectors.vm">
	<name>Channel Reader</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This connector allows Mirth to listen for incoming events from other channels in the same Mirth instance.</description>
	<clientClassName>com.webreach.mirth.connectors.vm.ChannelReader</clientClassName>
	<serverClassName>com.webreach.mirth.connectors.vm.VMConnector</serverClassName>
	<sharedClassName>com.webreach.mirth.connectors.vm.ChannelReaderProperties</sharedClassName>
	<library type="CLIENT" path="vm-client.jar" />
	<library type="SHARED" path="vm-shared.jar" />
	<library type="SERVER" path="vm-server.jar" />
	<transformers></transformers>
	<protocol>vm</protocol>
	<type>SOURCE</type>
	<enabled>true</enabled>
	<mule-properties>
		<property name="connector">com.webreach.mirth.connectors.vm.VMConnector</property>
		<property name="dispatcher.factory">com.webreach.mirth.connectors.vm.VMMessageDispatcherFactory</property>
		<property name="message.receiver">com.webreach.mirth.connectors.vm.VMMessageReceiver</property>
		<property name="message.adapter">com.webreach.mirth.connectors.vm.VMMessageAdapter</property>
		<property name="endpoint.builder">org.mule.impl.endpoint.ResourceNameEndpointBuilder</property>
	</mule-properties>
</connectorMetaData>