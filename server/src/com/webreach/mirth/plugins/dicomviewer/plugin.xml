<pluginMetaData path="com.webreach.mirth.plugins.dicomviewer">
	<name>DICOM Viewer</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This plugin provides DICOM attachment viewing capability in message browser.</description>
	<enabled>true</enabled>
	<extensionPoint>
		<name>DICOM Viewer</name>
		<type>ATTACHMENT_VIEWER</type>
		<className>com.webreach.mirth.plugins.dicomviewer.DICOMViewer</className>
		<mode>CLIENT</mode>
	</extensionPoint>
	<library path="dicomviewer-client.jar">
		<type>CLIENT</type>
	</library>
</pluginMetaData>