<pluginMetaData path="com.webreach.mirth.plugins.rtfviewer">
	<name>RTF Viewer</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This plugin provides RTF attachment viewing capability in message browser.</description>
	<enabled>true</enabled>
	<extensionPoint>
		<name>RTF Viewer</name>
		<type>ATTACHMENT_VIEWER</type>
		<className>com.webreach.mirth.plugins.rtfviewer.RTFViewer</className>
		<mode>CLIENT</mode>
	</extensionPoint>
	<library path="rtfviewer-client.jar">
		<type>CLIENT</type>	
	</library>
</pluginMetaData>