<connectorMetaData path="dicom">
	<name>DICOM Listener</name>
	<author>Mirth Corporation</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>2.1.0</mirthVersion>
	<url>http://www.mirthcorp.com</url>
	<description>This connector allows Mirth to listen for incoming messages over a standard TCP connection.</description>
	<clientClassName>com.mirth.connect.connectors.dimse.DICOMListener</clientClassName>
	<serverClassName>com.mirth.connect.connectors.dimse.DICOMConnector</serverClassName>
	<sharedClassName>com.mirth.connect.connectors.dimse.DICOMListenerProperties</sharedClassName>
	<library path="dicom-client.jar">
		<type>CLIENT</type>
	</library>
	<library path="dicom-shared.jar">
		<type>SHARED</type>
	</library>
	<library path="lib/slf4j-api-1.6.1.jar">
		<type>SHARED</type>
	</library>	
	<library path="lib/slf4j-log4j12-1.6.1.jar">
		<type>SHARED</type>
	</library>
	<library path="lib/dcm4che-core-2.0.25.jar">
		<type>SHARED</type>
	</library>
	<library path="dicom-server.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/jai_imageio.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/dcm4che-net-2.0.25.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/dcm4che-tool-dcmrcv-2.0.25.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/dcm4che-tool-dcmsnd-2.0.25.jar">
		<type>SERVER</type>
	</library>
	<library path="lib/dcm4che-filecache-2.0.25.jar">
		<type>SERVER</type>
	</library>
	<transformers></transformers>
	<protocol>dicom</protocol>
	<type>SOURCE</type>
	<enabled>true</enabled>
	<mule-properties>
		<property name="connector">com.mirth.connect.connectors.dimse.DICOMConnector</property>
		<property name="dispatcher.factory">com.mirth.connect.connectors.dimse.DICOMMessageDispatcherFactory</property>
		<property name="message.receiver">com.mirth.connect.connectors.dimse.DICOMMessageReceiver</property>
		<property name="message.adapter">com.mirth.connect.connectors.dimse.DICOMMessageAdapter</property>
		<property name="endpoint.builder">com.mirth.connect.connectors.file.FileEndpointBuilder</property>
	</mule-properties>
</connectorMetaData>