<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMapPUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN""http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="Script">	<select id="getScript" parameterClass="java.lang.String" resultClass="java.lang.String">		SELECT SCRIPT		FROM SCRIPT		WHERE ID = #id#	</select>	<insert id="insertScript" parameterClass="java.util.Map">		INSERT INTO		SCRIPT (ID, SCRIPT)		VALUES (#id#, #script#)	</insert>	<update id="updateScript" parameterClass="java.util.Map">		UPDATE SCRIPT		SET SCRIPT = #script#		WHERE ID = #id#	</update>	<delete id="deleteScript" parameterClass="java.lang.String">		DELETE FROM SCRIPT		<dynamic prepend="WHERE">			<isNotNull prepend="AND" property="id">				ID=#id#			</isNotNull>		</dynamic>	</delete></sqlMap>