<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMapPUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN""http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="Template">	<select id="getTemplate" parameterClass="java.lang.String" resultClass="java.lang.String">		SELECT TEMPLATE		FROM TEMPLATE		WHERE ID = #id#	</select>		<insert id="insertTemplate" parameterClass="java.util.Map">		INSERT INTO		TEMPLATE (ID, TEMPLATE)		VALUES (#id#, #template#)	</insert>		<update id="updateTemplate" parameterClass="java.util.Map">		UPDATE TEMPLATE		SET TEMPLATE = #template#		WHERE ID = #id#	</update>		<delete id="deleteTemplate" parameterClass="java.lang.String">		DELETE FROM TEMPLATE		<dynamic prepend="WHERE">			<isNotNull prepend="AND" property="id">				ID=#id#			</isNotNull>		</dynamic>	</delete></sqlMap>