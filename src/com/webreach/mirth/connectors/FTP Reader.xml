<connectorMetaData>
  <name>FTP Reader</name>
  <author>WebReach, Inc.</author>
  <pluginVersion>1.6.0</pluginVersion>
  <mirthVersion>1.6.0</mirthVersion>
  <url>http://www.mirthproject.org</url>
  <clientClassName>com.webreach.mirth.connectors.ftp.FTPReader</clientClassName>
  <sharedClassName>com.webreach.mirth.connectors.ftp.FTPReaderProperties</sharedClassName>
  <serverClassName>com.webreach.mirth.connectors.ftp.FtpConnector</serverClassName>
  <transformers>ByteArrayToString</transformers>
  <protocol>ftp</protocol>
  <type>LISTENER</type>
  <enabled>true</enabled>
  <mule-properties>
      <property name="connector">com.webreach.mirth.connectors.ftp.FtpConnector</property>
      <property name="dispatcher.factory">com.webreach.mirth.connectors.ftp.FtpMessageDispatcherFactory</property>
      <property name="message.receiver">com.webreach.mirth.connectors.ftp.FtpMessageReceiver</property>
      <property name="message.adapter">com.webreach.mirth.connectors.ftp.FtpMessageAdapter</property>
      <property name="endpoint.builder">org.mule.impl.endpoint.SocketEndpointBuilder</property>
  </mule-properties>
</connectorMetaData>