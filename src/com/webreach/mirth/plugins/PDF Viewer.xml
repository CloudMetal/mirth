<pluginMetaData>
  <name>PDF Viewer</name>
  <author>WebReach, Inc.</author>
  <pluginVersion>@mirthversion</pluginVersion>
  <mirthVersion>1.7.0</mirthVersion>
  <updateUrl><![CDATA[http://extensions.mirthproject.org/updates/?mirthversion=${mirthVersion}&version=${version}&name=${name}&serverid=${serverid}]]></updateUrl>
  <url>http://extensions.mirthproject.org</url>
  <description>This plugin provides attachment viewing capability in message browser</description>
  <enabled>true</enabled>
  <extensionPoint>
  	<name>PDF Viewer</name>
    <type>ATTACHMENT_VIEWER</type>
    <className>com.webreach.mirth.plugins.pdfviewer.PDFViewer</className>
    <mode>CLIENT</mode>
  </extensionPoint>
</pluginMetaData>