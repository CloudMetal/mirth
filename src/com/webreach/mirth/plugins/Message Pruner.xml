<pluginMetaData>
  <name>Message Pruner</name>
  <author>WebReach, Inc.</author>
  <pluginVersion>1.6.0</pluginVersion>
  <mirthVersion>1.6.0</mirthVersion>
  <updateUrl><![CDATA[http://extensions.mirthproject.org/updates/?mirthversion=${mirthVersion}&version=${version}&name=${name}&serverid=${serverid}]]></updateUrl>
  <url>http://extensions.mirthproject.org</url>
  <description>This plugin provides a message pruning function for messages stored in Mirth</description>
  <enabled>true</enabled>
  <extensionPoint>
  	<name>Message Pruner</name>
    <type>CLIENT_PANEL</type>
    <className>com.webreach.mirth.plugins.messagepruner.MessagePrunerClient</className>
    <mode>CLIENT</mode>
  </extensionPoint>
  <extensionPoint>
  	<name>Message Pruner</name>
    <type>SERVER_PLUGIN</type>
    <className>com.webreach.mirth.plugins.messagepruner.MessagePrunerService</className>
    <mode>SERVER</mode>
  </extensionPoint>
</pluginMetaData>